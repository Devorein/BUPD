{
  "name": "@bupd/validation",
  "version": "1.0.0",
  "description": "Validation package for BUPD",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "del-cli ./dist",
    "start": "node ./dist/index.js",
    "start:watch": "nodemon dist/index.js",
    "build": "tsc --sourceMap false",
    "build:watch": "tsc -w",
    "lint": "npx eslint \"./src\" --ext tsx,ts",
    "lint:fix": "npx eslint \"./src\" --ext tsx,ts --fix",
    "test": "npx jest --runInBand --config ./jest.config.js",
    "format": "npx prettier ./src/**/*.{ts,js} --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "yup": "^0.32.11"
  },
  "devDependencies": {}
}